print(max(a))
print(min(a)) #Prints a space bc thats the smallest ascii value in that string
print(min(b))

#Ascii values
print(ord('a'))
print(ord('A'))
print(ord('!'))
print(ord(' '))
print()
#Get character using ASCII
print(chr(33))
